{
  "name": "brisky-struct",
  "description": "An observable data structure",
  "version": "1.1.7-8ffa06c799bb1b5e8006a4d0e820b88526c6a739",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "build": "rollup -c rollup/build.js",
    "watch": "rollup -c rollup/build.js -w",
    "pretest": "npm run build && standard",
    "version-commit": "npm --no-git-tag-version version $(node -pe \"require('./package.json').version.split('-')[0]\")-$(git log -n 1 --pretty=format:'%H')",
    "publish-branch": "npm run version-commit && npm publish --tag $(git rev-parse --abbrev-ref HEAD | sed 's/\\//-/g')",
    "prepublish": "npm run build",
    "test": "node test/index.js",
    "dev": "rollup -c rollup/build.js -w & nodemon --harmony-async-await test/index.js",
    "cover": "nyc npm test",
    "travis": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/brisky-struct.git"
  },
  "browser": {
    "./src/methods/inspect": {}
  },
  "keywords": [
    "data structure",
    "vector",
    "functional",
    "memory efficient",
    "emitter",
    "subscriptions",
    "realtime",
    "observable"
  ],
  "nyc": {
    "include": [
      "dist/index.js"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "brisky-stamp": "^4.0.0",
    "string-hash": "^1.1.0"
  },
  "devDependencies": {
    "source-map-support": "^0.4.7",
    "nyc": "^10.0.0",
    "coveralls": "^2.11.9",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.1.3",
    "rollup": "^0.37.0",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^2.5.0",
    "standard": "^8.1.0",
    "tape": "4.6.0"
  },
  "author": "jim de beer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/brisky-struct/issues"
  },
  "homepage": "https://github.com/vigour-io/brisky-struct#readme",
  "contributors": [
    "Jim de Beer <jim@vigour.io>"
  ]
}
