{
  "name": "brisky-struct",
  "description": "An observable data structure",
  "version": "0.0.33",
  "main": "dist/brisky-struct.js",
  "module": "dist/brisky-struct.es.js",
  "jsnext:main": "dist/brisky-struct.es.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "test": "(ducktape; node test) | tap-spec && standard",
    "dev": "npm run watch & nodemon --harmony-async-await test | tap-spec",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/brisky-struct.git"
  },
  "keywords": [
    "data structure",
    "vector",
    "functional",
    "memory efficient",
    "subscriptions",
    "realtime",
    "observable"
  ],
  "dependencies": {
    "brisky-stamp": "^2.1.1",
    "monotonic-timestamp": "0.0.9"
  },
  "browser": {
    "./test/index.js": "./test/browser.js"
  },
  "devDependencies": {
    "buble": "0.15.1",
    "coveralls": "^2.11.9",
    "ducktape": "^1.0.0",
    "istanbul": "^0.4.3",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.1.3",
    "rollup": "^0.37.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^2.5.0",
    "standard": "^8.1.0",
    "tap-spec": "4.1.1",
    "tape": "4.6.0"
  },
  "author": "jim de beer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/brisky-struct/issues"
  },
  "homepage": "https://github.com/vigour-io/brisky-struct#readme",
  "contributors": [
    "Jim de Beer <jim@vigour.io>"
  ]
}
