{
  "name": "brisky-struct",
  "description": "An observable data structure",
  "version": "1.1.7-59333b32c30878c06af5b10ba5e0dd8640d2bf9e",
  "main": "dist/index.js",
  "scripts": {
    "build": "builder-boy src/index.js dist/index.js",
    "lint": "standard --fix",
    "version-commit": "npm --no-git-tag-version version $(node -pe \"require('./package.json').version.split('-')[0]\")-$(git log -n 1 --pretty=format:'%H')",
    "publish-branch": "npm run version-commit && npm publish --tag $(git rev-parse --abbrev-ref HEAD | sed 's/\\//-/g')",
    "prepublish": "npm run build",
    "test": "node test/index.js",
    "cover": "nyc npm test",
    "travis": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "precommit": "lint, test",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/brisky-struct.git"
  },
  "keywords": [
    "data structure",
    "vector",
    "functional",
    "memory efficient",
    "emitter",
    "subscriptions",
    "realtime",
    "observable"
  ],
  "nyc": {
    "include": [
      "dist/index.js"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "brisky-stamp": "^4.0.0",
    "string-hash": "^1.1.0"
  },
  "devDependencies": {
    "builder-boy": "latest",
    "source-map-support": "^0.4.7",
    "nyc": "^10.0.0",
    "coveralls": "^2.11.9",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.1.3",
    "standard": "^8.1.0",
    "tape": "4.6.0"
  },
  "author": "jim de beer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/brisky-struct/issues"
  },
  "homepage": "https://github.com/vigour-io/brisky-struct#readme",
  "contributors": [
    "Jim de Beer <jim@vigour.io>"
  ]
}
