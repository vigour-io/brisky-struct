{
  "name": "brisky-struct",
  "description": "An observable data structure",
  "version": "0.0.33",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "build": "rollup -c rollup/dist.js",
    "watch": "rollup -c rollup/dist.js -w",
    "pretest": "npm run build",
    "prepublish": "npm run build",
    "postinstall": "[ -d dist ] || npm run build || ( mkdir dist && yarn && npm run build );",
    "test": "standard && node test/index.js",
    "dev": "rollup -c rollup/dist.js -w & nodemon --harmony-async-await test/index.js",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigour-io/brisky-struct.git"
  },
  "keywords": [
    "data structure",
    "vector",
    "functional",
    "memory efficient",
    "subscriptions",
    "realtime",
    "observable"
  ],
  "dependencies": {
    "brisky-stamp": "^3.0.8"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "coveralls": "^2.11.9",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.1.3",
    "rollup": "^0.37.0",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^2.5.0",
    "standard": "^8.1.0",
    "tape": "4.6.0"
  },
  "author": "jim de beer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/brisky-struct/issues"
  },
  "homepage": "https://github.com/vigour-io/brisky-struct#readme",
  "contributors": [
    "Jim de Beer <jim@vigour.io>"
  ]
}
